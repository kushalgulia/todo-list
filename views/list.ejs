<%- include('header'); -%>


<div class="box">
  <h1 id="heading"> <%=heading%> </h1>
</div>

<div class="box">
  <% for(var v=0;v<itemList.length;v++){%>
  <%if(itemList[v]!==""){%>
  <div class="item">
    <input type="checkbox" name="" value="">
    <p><%= itemList[v]%></p>
  </div>

  <% } }%>

  <form class="item" action=<%=url %> method="post">
    <input type="text" name="newItem" placeholder="New Item" autocomplete="off" autofocus>
    <button type="submit" name="button">+</button>
  </form>
</div>


<%- include('footer'); -%>